<!--doctors.wxml-->
<!--头部start-->
<view class="header-dw">
  <view class="header-address">
    <text>{{name}}医生</text>
  </view>
</view>
<!--头部end-->
<!--照片start-->
<view class="">
  <image class="photos" src="{{'http://www.yyaai.com/uploads/doctor/'+ photo}}"></image>
</view>
<view class="near_clinics_title">
  <text>{{name}}医生</text> 
</view>
<!--照片end-->
<!--咨询预约start-->
<view class="adv-appoint" >
  <view class="adv" bindtap="adv">
    <image src="/image/icon/adv_pen.png"></image>
    <text>我要咨询</text>
  </view>
  <view class="appoint" bindtap="appoint">
    <image src="/image/icon/appoint_icon.png"></image>
    <text>马上预约</text>
  </view>
</view>
<!--咨询预约end-->
<!--个人简介start-->
<view class="profile">
  <view class="profile-title">
    <image src="/image/icon/approve_zhizhao.png"></image>
    <text>个人简介</text>
  </view>
  <view class="profile-content">
    <text>{{aboutme}}</text>
  </view>
</view>
<!--个人简介end-->

